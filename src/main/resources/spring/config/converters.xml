<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
	
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.AdminToStringConverter" />
		<bean class="converters.BudgetToStringConverter" />
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.CurriculumToStringConverter" />
		<bean class="converters.CustomerToStringConverter" />
		<bean class="converters.MaterialToStringConverter" />
		<bean class="converters.PainterToStringConverter" />
		<bean class="converters.RequestToStringConverter" />
		
		<bean class="converters.StringToActorConverter" />
		<bean class="converters.StringToAdminConverter" />
		<bean class="converters.StringToBudgetConverter" />
		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.StringToCurriculumConverter" />
		<bean class="converters.StringToCustomerConverter" />
		<bean class="converters.StringToMaterialConverter" />
		<bean class="converters.StringToPainterConverter" />
		<bean class="converters.StringToRequestConverter" />
	
	
	</util:list>
	
</beans>